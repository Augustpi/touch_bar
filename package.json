{
  "name": "vscode-touch-bar",
  "version": "0.0.4",
  "description": "Touchbar Setting",
  "main": "main.js",
  "scripts": {
    "test": "npm test",
    "publish": "vsce package",
    "pack": "vsce package"
  },
  "author": "Avni Onur Pehlivan @Augustpi",
  "publisher": "AvniOnurPehlivan",
  "license": "MIT",
  "repository": "https://github.com/Augustpi/touch_bar",
  "bugs": "https://github.com/Augustpi/touch_bar",
  "homepage": "https://github.com/Augustpi/touch_bar",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "editor.action.revealDefinition",
        "when": "editorFocus",
        "title": "→ƒ"
      },
      {
        "command": "editor.action.goToReferences",
        "when": "editorFocus",
        "title": "→Ref"
      },
      {
        "command": "workbench.action.terminal.openNativeConsole",
        "when": "editorFocus",
        "title": "⬓"
      },
      {
        "command": "workbench.action.showCommands",
        "when": "editorFocus",
        "title": ">_"
      },
      {
        "command": "editor.action.blockComment",
        "when": "editorHasMultipleSelections",
        "title": "/* */"
      },
      {
        "command": "editor.action.blockComment",
        "when": "editorHasSelection",
        "title": "//"
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "editor.action.revealDefinition",
          "when": "editorFocus",
          "title": "→ƒ"
        },
        {
          "command": "editor.action.goToReferences",
          "when": "editorFocus",
          "title": "->Ref"
        },
        {
          "command": "workbench.action.terminal.openNativeConsole",
          "when": "editorFocus",
          "title": "⬓"
        },
        {
          "command": "workbench.action.showCommands",
          "when": "editorFocus",
          "title": ">_"
        },
        {
          "command": "editor.action.blockComment",
          "when": "editorHasMultipleSelections",
          "title": "/* */"
        },
        {
          "command": "editor.action.blockComment",
          "when": "editorHasSelection",
          "title": "//"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "touchbar"
    }
  }
}