{
  "name": "vscode-touch-bar",
  "version": "0.0.1",
  "description": "Touchbar Setting",
  "main": "main.js",
  "scripts": {
    "test": "npm test",
    "publish": "vsce package",
    "pack": "vsce package"
  },
  "author": "Avni Onur Pehlivan @Augustpi",
  "publisher": "AvniOnurPehlivan",
  "license": "MIT",
  "repository": "https://github.com/Augustpi/touch_bar",
  "bugs": "https://github.com/Augustpi/touch_bar",
  "homepage": "https://github.com/Augustpi/touch_bar",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "editor.action.revealDefinition",
        "title": "->Def"
      },
      {
        "command": "editor.action.goToReferences",
        "title": "->Ref"
      },
      {
        "command": "workbench.action.terminal.openNativeConsole",
        "title": "iTerm"
      },
      {
        "command": "workbench.action.showCommands",
        "title": "CMD"
      },
      {
        "command": "ssen.touchbar.run",
        "title": "Run"
      },
      {
        "command": "ssen.touchbar.debug",
        "title": "Debug"
      },
      {
        "command": "typescript.restartTsServer",
        "title": "Restart TSServer"
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "editor.action.goToReferences",
          "group": "g2",
          "when": "editorHasReferenceProvider"
        },
        {
          "command": "editor.action.revealDefinition",
          "group": "g1",
          "when": "editorHasDefinitionProvider"
        },
        {
          "command": "workbench.action.terminal.openNativeConsole",
          "group": "g1"
        },
        {
          "command": "workbench.action.showCommands",
          "group": "g3"
        },
        {
          "command": "typescript.restartTsServer",
          "group": "g4"
        },
        {
          "command": "ssen.touchbar.run",
          "group": "g5",
          "when": "!inDebugMode"
        },
        {
          "command": "ssen.touchbar.debug",
          "group": "g6",
          "when": "!inDebugMode"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "SSen's Touchbar Config"
    }
  }
}